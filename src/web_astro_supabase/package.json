{
  "name": "party_inviter_frontend_using_astro",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest --config ./vitest.config.ts",
    "test:e2e": "npx playwright test --config ./playwright.config.ts",
    "test:e2e:localhost": "start-server-and-test dev http://localhost:4321 test:e2e",
    "test:e2e:prod": "cross-env PLAYWRIGHT_TEST_BASE_URL=https://party-invitations-public-demo.netlify.app npm run test:e2e",
    "e2ewtf": "npx playwright show-report e2e_test_results/html"
  },
  "dependencies": {
    "@astrojs/netlify": "^6.6.2",
    "astro": "^5.16.0"
  },
  "devDependencies": {
    "@astrojs/node": "^9.5.1",
    "@playwright/test": "^1.56.1",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "playwright-ctrf-json-reporter": "^0.0.26",
    "start-server-and-test": "^2.1.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  }
}
